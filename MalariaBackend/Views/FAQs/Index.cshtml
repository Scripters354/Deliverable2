@model IEnumerable<MalariaBackend.Models.FAQ>

@{
    ViewBag.Title = "Index";
}

<head>
    <link href="~/PortView/css/style.css" rel='stylesheet' type='text/css' />

    <link href="//fonts.googleapis.com/css?family=PT+Sans+Narrow:400,700" rel="stylesheet">
</head>

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FAQ_Question)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FAQ_Answer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.About_Malaria.Malaria_Description)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FAQ_Question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FAQ_Answer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.About_Malaria.Malaria_Description)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.FAQ_ID }) |
                @Html.ActionLink("Details", "Details", new { id = item.FAQ_ID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.FAQ_ID })
            </td>
        </tr>
    }

</table>
