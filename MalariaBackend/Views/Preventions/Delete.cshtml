@model MalariaBackend.Models.Prevention

@{
    ViewBag.Title = "Delete";
}
<head>
    <link href="~/PortView/css/style.css" rel='stylesheet' type='text/css' />

    <link href="//fonts.googleapis.com/css?family=PT+Sans+Narrow:400,700" rel="stylesheet">
</head>

<h2></h2>

<h3></h3>
<div>
    <h4>Details to be removed!</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Prevention Method
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Prevention_Method)
        </dd>


    </dl> 
    <br />

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Remove details" class="btn btn-default" /> 
           
        </div>
    }
</div>
